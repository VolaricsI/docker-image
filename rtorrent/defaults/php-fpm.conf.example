[global]
daemonize = no
;error_log = /tmp/php_error.log

[rutorrent]
listen = /tmp/php-fpm.sock
pm = dynamic
pm.max_children = 15
pm.start_servers = 1
pm.min_spare_servers = 1
pm.max_spare_servers = 6
chdir = /

user = abc
group = abc

listen.owner = abc
listen.group = abc


php_admin_flag[log_errors] = on
php_admin_value[error_log] = /dev/stdout
catch_workers_output = yes

